 '(speedbar-default-position (quote right))
 '(speedbar-tag-hierarchy-method (quote (speedbar-prefix-group-tag-hierarchy)))
 '(speedbar-use-images nil))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(default ((((class color) (min-colors 257)) (:foreground "#F8F8F2" :background "#272822" :family "Hack" :foundry "simp" :slant normal :weight normal :height 90 :width normal)) (((class color) (min-colors 89)) (:foreground "#F5F5F5" :background "#1B1E1C" :family "Hack" :foundry "simp" :slant normal :weight normal :height 90 :width normal)))))
